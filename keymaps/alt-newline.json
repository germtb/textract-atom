{
	"atom-workspace atom-text-editor:not([mini])": {
		"alt-o": "textract:line-down",
		"alt-shift-o": "textract:line-up",
		"alt-c": "textract:console-down",
		"alt-shift-c": "textract:console-up",
		"alt-d": "textract:debugger-down",
		"alt-shift-d": "textract:debugger-up",
		"cmd-shift-c": "textract:extract-const",
		"cmd-shift-v": "textract:extract-var"
	}
}
